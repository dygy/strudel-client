{
  "source": {
    "includePattern": ".+\\.(js(doc|x)?|mjs)$",
    "excludePattern": "node_modules|shift-parser|shift-reducer|shift-traverser|dist|examples|tools|website"
  },
  "plugins": ["plugins/markdown", "jsdoc/jsdoc-synonyms"],
  "opts": {
    "destination": "./out/",
    "recurse": true
  }
}
