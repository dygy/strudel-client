{
  "framework": "astro",
  "buildCommand": "pnpm run jsdoc-json && cp doc.json website/ && cd website && pnpm run build",
  "outputDirectory": "website/dist",
  "installCommand": "pnpm install",
  "devCommand": "cd website && npm run dev",
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}